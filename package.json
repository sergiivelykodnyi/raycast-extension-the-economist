{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "the-economist",
  "title": "The Economist",
  "description": "Read the latest story of The Economist",
  "icon": "command-icon.png",
  "author": "sergii",
  "categories": [
    "News"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "news",
      "title": "News",
      "description": "Get latest news.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.1",
    "@raycast/utils": "^1.12.4",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
